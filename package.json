{
  "author": "Jonas Thelemann",
  "contributors": [
    "Dario Ferderber <dario.ferderber@broj42.com>"
  ],
  "dependencies": {
    "@nuxt/kit": "^3.8.0",
    "@sindresorhus/slugify": "^2.2.1",
    "css-vars-ponyfill": "^2.4.8",
    "string-replace-loader": "^3.1.0"
  },
  "description": "Nuxt Cookie Control Module",
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@dargmuesli/nuxt-cookie-control": "link:",
    "@nuxt/module-builder": "0.5.5",
    "@nuxt/schema": "3.9.0",
    "@nuxtjs/eslint-config-typescript": "12.1.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.2",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "nuxt": "3.9.0",
    "prettier": "3.1.1",
    "typescript": "5.3.3",
    "vue": "3.3.13",
    "vue-tsc": "1.8.27",
    "webpack": "5.89.0"
  },
  "engines": {
    "node": ">=16"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    },
    "./runtime/*": {
      "import": "./dist/runtime/*.mjs",
      "require": "./dist/runtime/*.mjs",
      "types": "./dist/runtime/*.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "funding": {
    "url": "https://github.com/sponsors/dargmuesli"
  },
  "keywords": [
    "nuxt cookie control",
    "nuxt gdpr",
    "nuxt cookie consent",
    "nuxt gdpr consent"
  ],
  "license": "MIT",
  "main": "./dist/module.cjs",
  "maintainers": [
    "Jonas Thelemann"
  ],
  "name": "@dargmuesli/nuxt-cookie-control",
  "packageManager": "pnpm@8.13.1",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/dargmuesli/nuxt-cookie-control",
  "resolutions": {
    "@nuxt/kit": "3.9.0"
  },
  "scripts": {
    "build": "nuxt-module-build build",
    "lint": "eslint --ext .js,.ts,.vue . && nuxt typecheck",
    "lint:fix": "eslint --ext .js,.ts,.vue --fix . && nuxt typecheck --fix",
    "prepack": "pnpm build",
    "prepare": "husky install && nuxt-module-build prepare"
  },
  "type": "module",
  "types": "./dist/module.d.ts",
  "version": "7.2.1"
}
